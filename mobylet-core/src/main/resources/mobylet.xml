<?xml version="1.0" encoding="UTF-8"?>
<mobylet>
	<initializers>
		<chain>org.mobylet.core.initializer.impl.MobyletInitializerImpl</chain>
	</initializers>
	<device>
		<basedir>device/</basedir>
	</device>
	<emoji>
		<basedir>emoji/</basedir>
		<imagePath></imagePath>
	</emoji>
	<!--
	<cssExpand>true</cssExpand>
	-->
	<mobyletSession>
		<key>GUID</key>
		<timeout>30</timeout>
		<adapter>org.mobylet.core.session.impl.MobyletDistributeSessionAdapter</adapter>
		<distribution>
			<path>/mobyletSession</path>
			<method>POST</method>
			<parameter>s</parameter>
			<receiveHosts>
				<host name="host1">127.0.0.1:8080</host>
				<host name="host2">127.0.0.1:8080</host>
			</receiveHosts>
			<sendHosts>
				<host>127.0.0.1:8080</host>
				<host>127.0.0.1:8080</host>
			</sendHosts>
		</distribution>
	</mobyletSession>
</mobylet>